{
  "name": "cnpj",
  "version": "0.1.0",
  "description": "Format, validate and generate CNPJ numbers",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src -d dist",
    "postinstall": "npm run build",
    "test": "ava"
  },
  "keywords": [
    "cpnj",
    "validation",
    "brasil",
    "brazil"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gabrielizaias/cnpj.git"
  },
  "bugs": {
    "url": "https://github.com/gabrielizaias/cnpj/issues"
  },
  "author": "Gabriel Izaias <gabriel.izaias@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.22.0",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.26.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.0.4",
    "prettier": "^1.6.1"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "prettier": {
    "tabWidth": 2,
    "printWidth": 80,
    "useTabs": false,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "parser": "babylon"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  }
}
