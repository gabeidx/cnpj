  },
  "bugs": {
    "url": "https://github.com/gabrielizaias/cnpj/issues"
  },
  "author": {
    "name": "Gabriel Izaias",
    "email": "gabriel.izaias@gmail.com",
    "url": "https://izaias.co"
  },
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
